<!DOCTYPE html>
<head>

</head>
<body>
    
    <p><%= title %></p>
    <ul>
        <% items.forEach(function(item) { %>
            
          <li>
                       
            <img src="<%= item.picUrl %>" alt="">
            <br>
            <p> <%= item.text %> </p>
            <br>            

            <p>Gallery</p>
            <% item.gallery.forEach(function(g) { %>            
              <img src="<%= g.url %>" alt="">            
            <% }); %>

            

          </li>

          <hr/>
          
        <% }); %>
    </ul>
    

    
</body>

</html>